@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="page-header card">
    <div class="row align-items-end">
        <div class="col-lg-8">
            <div class="page-header-title">
                <i class="feather icon-plus-circle bg-c-blue"></i>
                <div class="d-inline">
                    <h5>Add New User Type</h5>
                    <span>You can add new user types from here!</span>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="card">

    <div class="card-header">
        <form>

            <div class="form-group row">
                <label class="col-sm-2 col-form-label">User Type ID</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="" readonly>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">User Type Name</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control form-control-success" id="inputSuccess1">
                    <div class="col-form-label"></div>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-7"></div>
                <div class="col-md-1">
                    <button class="btn waves-effect waves-light hor-grd btn-grd-info" style="margin-left: 13px;">Save</button>
                </div>
                <div class="col-md-1">
                    <button class="btn waves-effect waves-light hor-grd btn-grd-warning ">Cancel</button>
                </div>
                <div class="col-md-1">
                    <button class="btn waves-effect waves-light hor-grd btn-grd-danger ">Delete</button>
                </div>
            </div>
        </form>
    </div>
</div>
